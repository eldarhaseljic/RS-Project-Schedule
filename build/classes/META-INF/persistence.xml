<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="raspored" transaction-type="RESOURCE_LOCAL">
		<class>entiteti.Cas</class>
		<class>entiteti.Grupa</class>
		<class>entiteti.Korisnik</class>
		<class>entiteti.LocalDateAttributeConverter</class>
		<class>entiteti.LocalTimeAttributeConverter</class>
		<class>entiteti.Nastavnik</class>
		<class>entiteti.Predmet</class>
		<class>entiteti.Rezervacija</class>
		<class>entiteti.Sala</class>
		<class>entiteti.Semestar</class>
		<class>entiteti.Student</class>
		<class>entiteti.Usmjerenje</class>
		<class>entiteti.Zgrada</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:derby:/home/eldar/MyDB;create=true"/>
			<property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.EmbeddedDriver"/>
			<property name="javax.persistence.jdbc.password" value=""/>
			<property name="javax.persistence.jdbc.user" value=""/>
		</properties>
	</persistence-unit>
</persistence>
